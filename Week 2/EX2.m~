clear
close all
%input x
x = linspace(0,1,101);

%input variables for Re's du/dx's
ReL = [1e6 1e7 1e8];
grad = [-.2 0 .2];

%iterate a matrix contains ue's in different Re
for i = 1:length(x);
    for j = 1:length(grad);
        ue(j,i) = grad(j)*x(i)+1;
    end
end

%iterate for theta/L
for i=1:length(x);
    theta(i) = sqrt(.45/ReL*(ue(i))^-6*ueintbit(0,ue(1),x(i),ue(i));
end

%plot of theta/L
hold on
plot(x,theta);
xlabel('non dimensional position x/L');
ylabel('non dimensional displacement theta/L');

%calculate blasius
thetab = 0.664/ReL^.5 .* (x).^.5;
%plotting blasius
plot(x,thetab);
legend('analytical','Blasius','location','Southeast');